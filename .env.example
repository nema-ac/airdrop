# SOL NEMA Airdrop Environment Variables
# Copy this file to .env and fill in your actual values

# REQUIRED: Private key of the source wallet (Base58 encoded)
# Keep this secret! Never commit the actual private key to version control
AIRDROP_PRIVATE_KEY=your_base58_encoded_private_key_here

# REQUIRED: SPL token mint address for SOL NEMA tokens
TOKEN_MINT_ADDRESS=your_token_mint_address_here

# OPTIONAL: Solana RPC endpoint (defaults to mainnet-beta if not set)
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# OPTIONAL: Path to CSV file containing airdrop data
# Defaults to data/sol_nema_airdrop.csv if not set
CSV_FILE_PATH=data/sol_nema_airdrop.csv

# OPTIONAL: Batch processing configuration
BATCH_SIZE=10
BATCH_DELAY=1.0

# OPTIONAL: RPC rate limiting configuration
RPC_CHECK_DELAY=1.0
RPC_CHECK_BATCH=3

# OPTIONAL: Retry configuration
MAX_RETRIES=3

# OPTIONAL: Logging configuration
LOG_LEVEL=INFO

# OPTIONAL: Token configuration
TOKEN_DECIMALS=6

# OPTIONAL: Progress file configuration
# Will be automatically generated based on phase if not set
PROGRESS_FILE=airdrop_phase1_progress.json